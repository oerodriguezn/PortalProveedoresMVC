@using PortalProveedoresMVC.Models;
@{
    Layout = null;
    BioRetencionFacturas Retencion = (BioRetencionFacturas)ViewBag.Retencion;
    BioProveedores Proveedor = (BioProveedores)ViewBag.Proveedor;
    }
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Orden de Compra</title>
   
</head>
<body>
    <div>
        <table width="100%" cellspacing="0" cellpadding="0">
            <tr>
                <td style="width: 48%" align="center">
                    <img src="~/Content/Images/LogoODC.png" /><br />
